<html>
    <head>
        <title>Accounts</title>
    </head>
    <body>
        <h1>Accounts</h1>
        <table border="1">
            <tr><th>account ID</th><th>Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><%= result[i].account_id%></td>
                <td><a href="/account/?account_id=<%= result[i].account_id %>"><%= result[i].email %></a></td>

            </tr>
            <% } %>
        </table>
    </body>

</html>