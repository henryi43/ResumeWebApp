<html>
<head>
    <title>Add New Skill</title>
</head>
<body>
<h1>Add New Skill</h1>
<form action="/skill/insert" method="get">
    skill Name: <input id="skill_name" name="skill_name" type="text" />
    <br />
    <br />
    skill
    <select id="skill_id" name="skill_id">
        <% for(var i=0; skill.length > i; i++ ) { %>
        <option value="<%= skill[i].skill_id %>"><%= skill[i].description%></option>
        <% } %>
    </select>

    <input type="submit" />

</form>
</body>

</html>